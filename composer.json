{
    "name": "dcorroyer/mybudget",
    "description": "A simple Symfony project to manage your mensual budget.",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.15",
        "symfony/console": "~6.3.0",
        "symfony/dotenv": "~6.3.0",
        "symfony/flex": "^2",
        "symfony/form": "~6.3.0",
        "symfony/framework-bundle": "~6.3.0",
        "symfony/runtime": "~6.3.0",
        "symfony/security-bundle": "~6.3.0",
        "symfony/twig-bundle": "~6.3.0",
        "symfony/validator": "~6.3.0",
        "symfony/yaml": "~6.3.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "enlightn/security-checker": "^1.10",
        "ergebnis/composer-normalize": "^2.33",
        "friendsoftwig/twigcs": "^6.2",
        "phpro/grumphp": "^2.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^9.5",
        "shipmonk/phpstan-rules": "^2.6",
        "slam/phpstan-extensions": "^6.0",
        "spaze/phpstan-disallowed-calls": "^2.15",
        "symfony/browser-kit": "~6.3.0",
        "symfony/css-selector": "~6.3.0",
        "symfony/maker-bundle": "^1.50",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "~6.3.0",
        "symfony/web-profiler-bundle": "~6.3.0",
        "symplify/easy-coding-standard": "^11.5",
        "zenstruck/foundry": "^1.34"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpro/grumphp": false,
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
